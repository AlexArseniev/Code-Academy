#include <stdio.h>

int main(){
    char str[] = "Hello"; 
    char strX[6] = {0,0,0,0,0,0};
    short i = 0;

    while (str[i] != '\0'){
        if(str[i] == 'l') {
            strX[i] = 'x';
        } else{
            strX[i] = str[i];
        }
        i++;
    }
    
    printf("%s", strX);

    return 0;
}
