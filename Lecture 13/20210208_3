/*
Задача 3. Дефинирайте и инициализирайте тримерен масив с по 5
елемента (5 x 5 x 5).
*/

#include <stdio.h>

int main(void){

    int o = 0;

    int array[5][5][5] = {
        {{o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}},
        {{o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}},
        {{o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}},
        {{o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}},
        {{o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}, {o++,o++,o++,o++,o++}},
    };

    int i = 0, j = 0, h = 0;

    for(i; i < 5; i++){
        for(j; j < 5; j++){
            for (h; h < 5; h++){
                printf("[%d][%d][%d] = %d\n", i,j,h, array[i][j][h]);
            }
            h = 0;
        }
        j = 0;
    }
    return 0;
}
