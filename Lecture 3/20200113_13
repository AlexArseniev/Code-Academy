#include <stdio.h>

int main(){
    char a = 'a';
    int b = 102;
    long c = 5000;
    float d = 6;
    double e = -48.907;

    printf("%d\n", sizeof(a));
    printf("%c\n\n", a);
    printf("%d\n", sizeof(b));
    printf("%i\n\n", b);
    printf("%d\n", sizeof(c));
    printf("%d\n\n", c);
    printf("%d\n", sizeof(d));
    printf("%f\n\n", d);
    printf("%d\n", sizeof(e));
    printf("%lf\n", e);

    return 0;
}
