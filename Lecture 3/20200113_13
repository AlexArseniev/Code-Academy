#include <stdio.h>

int main(){
    float a = 4.9876543;
    double b = 7.123456789012345678890;
    long double c = 18398458438583853.28;
    long double d = 18398458438583853.39875937284928422L;
    
    printf("%.7f \n", a);
    printf("%.15f\n", b);
    printf("%ld\n", 0x415D4CEBA5862D.1C);
    __mingw_printf("%17.17Lf\n", d);

   return 0; 
}
